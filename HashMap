package Search;

import java.util.HashMap;

public class Sch1 {

    public static void main(String[] args) {
        HashMap<String, String> map = new HashMap<>();

        String 아침 = "아침"; String 점심 = "점심"; String 저녁 = "저녁";
        map.put(아침, "사과");
        map.put(점심, "수육");
        map.put(저녁, "짬뽕");

        System.out.println(map.get(점심));
    }

